<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:th="http://www.thymeleaf.org"
	xmlns:sec="http://www.thymeleaf.org/thymeleaf-extras-springsecurity3">
<body>

	<th:block  th:fragment="table">
		<div class="panel panel-inverse">
			<div class="panel-heading">
				<div class="panel-heading-btn">
					<a href="javascript:;" th:title="#{icon.reduire}"
						class="btn btn-xs btn-icon btn-circle btn-warning"
						data-click="panel-collapse"><i class="fa fa-minus"></i></a>
				</div>
				<h4 class="panel-title">
					<th:block th:text="#{referentiel.secteur.list}"></th:block>
				</h4>
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-md-12">
						<div id="content" th:content="content"></div>
					</div>
				</div>
			</div>
		</div>
	</th:block>



	<th:block th:fragment="breadcrumb(niveau1,niveau2)">
		<ol class="breadcrumb pull-left">
			<li th:text="#{${niveau1}}" ></li>
			<li th:if="${#strings.contains(niveau2,'.')}" th:text="#{${niveau2}}" ></li>
			<li th:if="${!#strings.contains(niveau2,'.')}" th:text="${niveau2}" ></li>
		</ol>
	</th:block>

<th:block th:fragment="localisation-panel">
<!-- #modal-message -->
		<div class="modal modal-message modal-localisation fade" id="modal-localisation">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal"
							aria-hidden="true">×</button>
					<h4 class="modal-title">	<i class="fa fa-map-marker"></i> <span class="gold"> Localisation</span></h4>
					</div>
					<div class="modal-body">
						<div class="embed-responsive embed-responsive-16by9">
							<iframe id="iFrameLocalisation" class="embed-responsive-item sig-iframe"
								src=""></iframe>
						</div>
					</div>
					<div class="modal-footer"></div>
				</div>
			</div>
		</div>
</th:block>
<th:block th:fragment="document-panel">
		<div class="theme-panel" ng-show="selected!=null">
			<a href="javascript:;" data-click="theme-panel-expand" ng-click="showDocumentsAssocies('documentsTable')"
				class="theme-collapse-btn "><i class="fa fa-folder"></i></a>
			<div class="theme-panel-content">
				<h5 class="m-t-0"><th:block th:text="#{document.table.documents}"></th:block></h5>
				<div class="divider"></div>
				<div class="row">
					<div class="col-md-12">
						<table id="documentsTable" class="table table-striped table-bordered"
							 th:attr="ng-init='initListDocuments();'">
							<thead>
								<tr>
									<th class="noVis"></th>
									<th><th:block th:text="#{document.table.designation}"></th:block></th>
									<th><th:block th:text="#{document.table.type}"></th:block></th>
									<th><th:block th:text="#{document.table.Date}"></th:block></th>
									<th><th:block th:text="#{document.table.Observation}"></th:block></th>
								</tr>
							</thead>
						</table>
					</div>
				</div>
			</div>
		</div>

	</th:block>
	<th:block th:fragment="localisation-modal">
	<!-- #modal-message -->
		<div class="modal modal-message modal-localisation fade" id="modal-localisation">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal"
							aria-hidden="true">×</button>
					<h4 class="modal-title">	<i class="fa fa-map-marker"></i> <span class="gold"> Localisation</span></h4>
					</div>
					<div class="modal-body">
						<div class="embed-responsive embed-responsive-16by9">
							<iframe id="iFrameLocalisation" class="embed-responsive-item sig-iframe"
								src=""></iframe>
						</div>
					</div>
					<div class="modal-footer"></div>
				</div>
			</div>
		</div>
	</th:block>
	<!-- begin theme-panel -->
	<th:block th:fragment="theme-panel">
		<div class="theme-panel" ng-controller="documentController" th:attr="ng-init='init();'">
			<a href="javascript:;" data-click="theme-panel-expand"
				class="theme-collapse-btn"><i class="fa fa-file-o"></i></a>
			<div class="theme-panel-content">
				<h5 class="m-t-0">Color Theme</h5>
				<div class="divider"></div>
				<div class="row">
					<div class="col-md-12">
						
					</div>
				</div>
			</div>
		</div>

	</th:block>
</body>
</html>