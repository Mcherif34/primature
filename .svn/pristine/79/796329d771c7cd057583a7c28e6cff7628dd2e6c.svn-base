.mediatheque-container {
  position: relative;
  width: 100%;
  height: 160px;
}

.mediatheque-container.display {
  height: 500px;
}
.mediatheque-container.display img{
  height: 500px;
}
.mediatheque-container.display video{
  height: 500px;
}

.col-div {
    padding: 4px 2px;
}

.mediatheque-container .overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0);
  transition: background 0.5s ease;
}

.mediatheque-container:hover .overlay {
  display: block;
  background: rgba(0, 0, 0, .3);
}

.mediatheque-container img {
  position: absolute;
  width: 100%;
  height: 160px;
  object-fit: cover;
  object-position: center;
}
.mediatheque-container video {
  position: absolute;
  width: 100%;
  height: 160px;
/*   object-fit: cover; */
  object-position: center;
}

.mediatheque-container .title, .mediatheque-container .title-video {
  position: absolute;
  width: 100%;
  left: 0;
  top: 80px;
  font-weight: 500;
  font-size: 20px;
  text-align: center;
  color: white;
  z-index: 1;
  transition: top .5s ease;
}

.mediatheque-container:hover .title {
  top: 50px;
}
.mediatheque-container:hover .title-video {
  top: 80px;
}

.mediatheque-container .button-display {
  position: absolute;
  width: 100%;
  left:0;
  top: 110px;
  text-align: center;
  opacity: 0;
  transition: opacity .35s ease;
}

.mediatheque-container .button-display a {
  width: 100%;
  padding: 8px 28px;
  text-align: center;
  color: white;
  cursor: pointer;
  border: solid 2px white;
  z-index: 1;
}
.mediatheque-container .button-display a:hover {
  text-decoration: none;
}

.mediatheque-container:hover .button-display {
  opacity: 1;
}

.mediatheque-container .btn-add {
  position: absolute;
  width: 100%;
  left:0;
  top: 65px;
  text-align: center;
  transition: opacity .35s ease;
}

.mediatheque-container .btn-add a {
  width: 100%;
  text-align: center;
  color: black;
  cursor: pointer;
  z-index: 1;
}

.mediatheque-container:hover .btn-add a{
  color: orange;
  text-shadow: 1px 1px 20px black;
}
.mediatheque-container .btn-add a i {
  font-size: 40px;
}


