.fab-actions {
  position: relative;
  text-align: center;
}

.fab-actions:hover .fab-label {
    opacity: 0;
    transition: opacity .5s .125s ease-out;
}
.fab-actions:hover .fab-icon {
    border-radius: 1em;
    margin: 0 2px;
}


.fab-label {
  background-color: #43515a;
  line-height: 20px;
  border-radius: 1rem;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  opacity: 1;
  pointer-events: none;
  transition: opacity .2s .15s ease-out;
}
.fab-label.green {
  background-color: #6C9B51;
}
.fab-label.blue {
  background-color: #43515a;
}

.fab-icon {
  border-radius: 0;
  cursor: pointer;
  display: inline-block;
  height: 20px;
  width: 20px;
  line-height: 20px;
  margin: 0 -.3em;
  transition:
    background-color .2s ease-out,
    border-radius .2s .15s ease-out,
    margin .2s .15s ease-out;
}
.fab-small {
	font-size: 10px;
}
.fab-icon.fab-first {
    border-bottom-left-radius: 1em;
    border-top-left-radius: 1em;
    margin-left: 0;
}
  
.fab-icon.fab-last {
    border-bottom-right-radius: 1em;
    border-top-right-radius: 1em;
    margin-right: 0;
}
  
  
.fab-actions a {
    color: white !important;
}


.fab-default {
  background-color: #b6c2c9;
}
.fab-primary {
  background-color: #49b6d6;
}
.fab-warning {
