.sidemenu {
  text-align: center;
  position: fixed;
/*   top: 27%; */
  top: 240px;
  left: 50%;
  transform: translate(-50%, -60%);
}
.sidemenu h1 {
  font-weight: 600;
  font-size: 2.4em;
  color: #303030;
  margin-bottom: 0.5em;
}
.sidemenu p {
  color: #999;
  font-weight: 400;
  font-size: 1.2em;
}
.sidemenu a {
  color: #4183d7;
  text-decoration: none;
  font-weight: 600;
  padding-bottom: 2px;
  border-bottom: 2px dotted #4183d7;
  transition: all 0.25s ease;
}
.sidemenu a:hover {
  color: #d2527f;
  border-color: #d2527f;
}





.sidemenu ul {
  list-style: none;
  position: absolute;
/*   top: 50%; */
  left: 0%;
/*   transform: translateY(-90%); */
}
.sidemenu ul li a {
  display: block;
  margin-left: 10px;
  height: 40px;
  width: 60px;
  border-radius: 0 25px 25px 0;
  border: 2px solid #000;
  background: #fff;
/*   margin-bottom: 0.5em; */
  transition: all 0.4s ease;
  color: #2980b9;
  text-decoration: none;
/*   line-height: 60px; */
  position: relative;
}
.sidemenu ul li a:hover {
  cursor: pointer;
  width: 190px;
  color: #fff;
}
.sidemenu ul li a.large:hover {
  cursor: pointer;
  width: 250px;
  color: #fff;
}
.sidemenu ul li a:hover span {
  left: 0;
}
.sidemenu ul li a span {
  padding-left: 30px;
  position: absolute;
  left: -150px;
  transition: left 0.4s ease;
  top: 50%;
  transform: translateY(-50%);
}
.sidemenu ul li a i {
  position: absolute;
  top: 50%;
  right: 20px;
  transform: translateY(-50%);
  font-size: 1em;
}
.sidemenu ul li a img {
  position: absolute;
  top: 50%;
  right: 15px;
  transform: translateY(-50%);
  font-size: 1em;
}



